{
  "name": "socket.io-prometheus-tracker",
  "version": "4.4.1-alpha.1",
  "description": "Prometheus metrics tracker for socket.io servers.",
  "main": "./build/index.js",
  "scripts": {
    "check:types": "tsc --noEmit",
    "doc": "del-cli \"./docs/**/*\" && typedoc src/index.ts",
    "prebuild": "npm run doc && del-cli \"./build/**/*\"",
    "build:source": "tsc -p tsconfig.dist.json",
    "build": "npm run prebuild && npm run build:source",
    "lint": "./node_modules/.bin/eslint \"./src/**/*.ts\" --color",
    "lint:fix": "./node_modules/.bin/eslint \"./src/**/*.ts\" --color --fix",
    "precommit-message": "echo Executing pre-commit checks... && exit 0",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsau-/socket.io-prometheus-tracker.git"
  },
  "keywords": [
    "prometheus",
    "socket.io",
    "socketio",
    "metrics",
    "tracker"
  ],
  "author": "Jon Saunders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsau-/socket.io-prometheus-tracker/issues"
  },
  "homepage": "https://github.com/jsau-/socket.io-prometheus-tracker#readme",
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "cross-env": "7.0.3",
    "del-cli": "4.0.1",
    "eslint": "7.26.0",
    "express": "4.17.3",
    "jest-html-reporter": "3.4.2",
    "node-notifier": "10.0.1",
    "pre-commit": "1.2.2",
    "prettier": "2.5.1",
    "socket.io-client": "4.4.1",
    "ts-jest": "27.1.3",
    "typedoc": "0.22.11",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "prom-client": "^12.0.0",
    "socket.io": "^4.4.1"
  },
  "pre-commit": [
    "precommit-message",
    "lint",
    "build"
  ]
}
